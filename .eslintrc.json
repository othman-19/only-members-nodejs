{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-shadow": "off",
        "arrow-parens": ["error", "as-needed"],
        "no-unused-vars": ["error", { "vars": "all", "args": "none"}]
    }
}
