<% if (messages.error) { %>
  <p><%= messages.error %></p>
<% } %>

<% if (locals.errors) { %>
    <ul>
      <% locals.errors.forEach(function(error){ %>
        <li> <strong>Warning!</strong> <%= error.msg %> </li>
      <% }); %>
  </ul>
<% } %>

<% if (messages.success) { %>
  <p><%= messages.success %></p>
<% } %>
<% if (messages.info) { %>
  <p><%= messages.info %></p>
<% } %>
<% if (messages.danger) { %>
  <p><%= messages.danger %></p>
<% } %>
